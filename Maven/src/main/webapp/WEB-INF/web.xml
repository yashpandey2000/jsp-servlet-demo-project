<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>stdfilter</filter-name>
  	<display-name>stdfilter</display-name>
  	<description></description>
  	<filter-class>co.in.control.stdfilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>stdfilter</filter-name>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>
  
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>co.in.control.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>welcomectl</servlet-name>
  	<display-name>welcomectl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.welcomectl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginctl</servlet-name>
  	<display-name>loginctl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.loginctl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>fpctl</servlet-name>
  	<display-name>fpctl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.fpctl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>studentctl</servlet-name>
  	<display-name>studentctl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.studentctl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>studentgetctl</servlet-name>
  	<display-name>studentgetctl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.studentgetctl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Emailctl</servlet-name>
  	<display-name>Emailctl</display-name>
  	<description></description>
  	<servlet-class>co.in.control.Emailctl</servlet-class>
  </servlet>
 
  
 
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>welcomectl</servlet-name>
  	<url-pattern>/welcomectl.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginctl</servlet-name>
  	<url-pattern>/loginctl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>fpctl</servlet-name>
  	<url-pattern>/fpctl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>studentctl</servlet-name>
  	<url-pattern>/studentctl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>studentgetctl</servlet-name>
  	<url-pattern>/studentgetctl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Emailctl</servlet-name>
  	<url-pattern>/Emailctl</url-pattern>
  </servlet-mapping>
 
</web-app>

